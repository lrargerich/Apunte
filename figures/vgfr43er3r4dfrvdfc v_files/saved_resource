function _indexOf(elem){
    var i = this.length;
    while(i--){
        if(this[ i ] === elem) break;
    }
    return i;
}

function loadOptimizelyData(a){
    //var dataLayer = window['dataLayer'] = window['dataLayer'] || [];
    var requireGoogleAnalyticsExperimentActivation = false;
    var optimizelyBucketSlotDefault = 5;
        
    var allExp = a.allExperiments || [], key;
    var hasown = Object.prototype.hasOwnProperty;

    var enabled, ga_enabled, names = {};
    var experiment_mapping = {}, active;

    for(key in allExp){
        if(hasown.call(allExp, key)){
            enabled = hasown.call(allExp[key], 'enabled');
            ga_enabled = hasown.call(allExp[key], 'google_analytics');
            names.slot = ga_enabled ? allExp[key].google_analytics.slot : optimizelyBucketSlotDefault;
            names.exp = allExp[key].name;
            names.vari = a.variationNamesMap[key] || null;

            ga_enabled = enabled && (ga_enabled || (requireGoogleAnalyticsExperimentActivation === false));

            experiment_mapping[ key ] = { // Collect meaningful Optimizely settings for ALL experiments
                'enabled' : enabled,
                'slot': (names.slot),
                'name': (names.exp),
                'variation': (names.vari),
                'bucketed': (names.vari !== null),
                'active': (active = !!(~ _indexOf.call(a.activeExperiments, key))),
                'state': active ? 'active' : 'inactive'
            };
        } 
    }

    
    var concatEvents = "";
    for(key in experiment_mapping){
        
        if(hasown.call(experiment_mapping, key) && experiment_mapping[key]['bucketed']){
            console.log("Optimizely", "optimizely_bucket: "+experiment_mapping[ key ]['name'], experiment_mapping[ key ]['variation'], true);
            var test =  experiment_mapping[ key ]['name']+" "+experiment_mapping[ key ]['variation'] +", ";
            //  window[tracker](tracker2+'set', 'dimension137', test);
            concatEvents+=test;
            TAGX.Utils.sendGA({ 
                hit: {
                    hitType: 'event',
                    eventCategory: 'Optimizely',
                    eventAction: "optimizely_bucket: "+experiment_mapping[ key ]['name'], 
                    eventLabel: experiment_mapping[ key ]['variation']
                },
                nonInteraction: true
            });
        };
        
    }
    console.log("Optimizely", "optimizely_loaded", true)
    concatEvents = concatEvents.substr(0, concatEvents.length-2);
    TAGX.Utils.sendGA({ 
        hit: {
            hitType: 'event',
            eventCategory: 'Optimizely',
            eventAction: 'optimizely_loaded'
        },
        customDimensions: {
            dimension136: concatEvents
        },
        nonInteraction: true
    });

}
if (a) console.log(a);

if (a) loadOptimizelyData(a);
else {
    c = 0;
    t = setInterval(function(){
        if(50 < c++){
            clearInterval(t);
        } else {
            if(w[o]){
                loadOptimizelyData(w[o]);
                clearInterval(t);
            }
        }
    }, 50);
}


